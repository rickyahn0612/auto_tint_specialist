#sub-page-header
  .container
    %h1 Contact Us
    %p Home/Contact Us

#map
  %iframe{frameborder: "0", height: "450", src: "https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3319.044971912416!2d-117.9972571!3d33.707785200000004!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x80dd268d62bb116f%3A0xd6c9941e563ca5b3!2s7522+Slater+Ave+%23115!5e0!3m2!1sen!2sus!4v1399055584744", style: "border:0", width: "100%"}

#contact-us-container
  .container
    #contact-us-form-container
      %br
      %form#test{action: "send.php", method: "post"}
        %fieldset
          %legend Contact Form
          %div
            %label{for: "name_Req"}
              Name
              %strong *
            %input#name_Req{name: "name", title: "Required! Please enter your name", type: "text"}
          %div
            %label{for: "contact_Req_Email"}
              E-mail
              %strong *
            %input#contact_Req_Email{name: "email", title: "Required! Please enter a valid email address", type: "text"}
          %div
            %label{for: "telephone_Tel"} Telephone
            %input#telephone_Tel{name: "telephone", title: "Please enter a valid telephone number", type: "text"}
          %div
            %label{for: "message_Req"}
              Message
              %strong *
            %textarea#message_Req{cols: "20", name: "message", rows: "5", title: "Required! Please enter your message"}
          %div
            %input.button{type: "submit", value: "Send"}






